        <activity
            android:name=".pages.RegistrationPage"
            android:exported="false" />
        <activity
            android:name=".pages.LogInPage"
            android:exported="false" />
        <activity
            android:name=".pages.YouPage"
            android:exported="false" />
        <activity
            android:name=".pages.UserVideos"
